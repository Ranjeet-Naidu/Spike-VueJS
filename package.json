{
  "name": "spike-vue",
  "description": "Spike vue capability",
  "version": "1.0.0",
  "author": "Ranjeet Naidu",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "rm -rf .cache && rm -rf dist && parcel src/index.html -p 1234",
    "build": "rm -rf .cache && rm -rf dist && parcel build src/index.html",
    "test": "jest --watch"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "nanoid": "^1.0.2",
    "vee-validate": "^2.0.6",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.16.0",
    "vuetify": "^1.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.13",
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-vue": "^4.3.0",
    "jest": "^22.4.3",
    "jest-vue-preprocessor": "^1.4.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "1.7.0",
    "postcss-modules": "^1.1.0",
    "vue-template-compiler": "^2.5.16"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "/src/**/*.{js}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
